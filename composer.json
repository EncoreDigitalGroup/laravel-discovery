{
    "name": "encoredigitalgroup/php-package-template",
    "type": "library",
    "authors": [
        {
            "name": "Marc Beinder",
            "email": "marc.beinder@encoredigitalgroup.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "neronmoon/scriptsdev": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "require": {
        "php": "^8.3",
        "encoredigitalgroup/stdlib": "^2",
        "illuminate/console": "^11|^12",
        "illuminate/support": "^11|^12",
        "nikic/php-parser": "^5.4",
        "phpgenesis/logger": "1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "larastan/larastan": "^3",
        "laravel/framework": "^11|^12",
        "neronmoon/scriptsdev": "^0.1.9",
        "orchestra/testbench": "^10",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-drift": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpgenesis/devutilities": "^1.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^2",
        "rector/rector": "^2",
        "tightenco/duster": "^3.0",
        "tomasvotruba/cognitive-complexity": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EncoreDigitalGroup\\LaravelDiscovery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "EncoreDigitalGroup\\LaravelDiscovery\\Providers\\DiscoveryServiceProvider"
            ],
            "dont-discover": []
        },
        "scripts-dev": {
            "post-update-cmd": [],
            "post-autoload-dump": [
                "vendor/bin/testbench package:discover"
            ]
        }
    }
}
